@model PaskalaRitenis.Models.NewsModel

@{
    ViewBag.Title = "Delete";
}


<h3>Vai jus tiešām gribat dzēst šo?</h3>
<fieldset>
    <div class="display-label">
         @Html.DisplayNameFor(model => model.Info)
    </div>
    <div class="display-field">
        @Html.Raw(Model.Info)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" class="btn btn-default" value="Dzēst" /> |
        @Html.ActionLink("Atpakaļ pie Ziņu saraksta", "Index")
    </p>
}
